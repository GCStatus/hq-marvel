{
  "name": "hq-marvel",
  "version": "0.0.1",
  "description": "Application for Marvel's informations.",
  "main": "./src/main.tsx",
  "scripts": {
    "start": "concurrently \"npm:dev\" \"npm:storybook\"",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "semantic-release": "semantic-release",
    "format": "prettier --write ./src && echo 'Formatting complete! Include any changes on your commit.'",
    "lint": "eslint --fix ./src && echo 'Lint complete.'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "echo 'Install jest later.'",
    "type-check": "tsc && echo 'Type check complete.'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "super",
    "superheroes",
    "heroes",
    "marvels",
    "marvel"
  ],
  "author": "Felipe Oliveira",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.1.11",
    "@vitejs/plugin-react": "^4.7.0",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.23.7",
    "hamburger-react": "^2.5.2",
    "lodash": "^4.17.21",
    "motion": "^12.23.7",
    "nprogress": "^0.2.0",
    "react": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "sanitize-html": "^2.17.0",
    "styled-components": "^6.1.19",
    "tailwindcss": "^4.1.11",
    "use-debounce": "^10.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.1",
    "@faker-js/faker": "^9.9.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.0.18",
    "@storybook/addon-themes": "^9.0.18",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.20",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-window": "^1.8.8",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.0.18",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "netlify-cli": "^22.3.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "storybook": "^9.0.18",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": false
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "overrides": {
    "@rollup/rollup-android-arm-eabi": "0.0.0"
  }
}
